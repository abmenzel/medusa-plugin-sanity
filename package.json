{
	"name": "medusa-plugin-sanity",
	"version": "1.0.2",
	"description": "Sanity CMS plugin for Medusa",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/abmenzel/medusa-plugin-sanity"
	},
	"author": {
		"name": "Alexander Menzel",
		"email": "alexander@hybl.dk",
		"url": "https://hybl.dk"
	},
	"license": "MIT",
	"keywords": [
		"medusa-plugin",
		"medusa-plugin-cms",
		"medusa-plugin-sanity"
	],
	"scripts": {
		"prepare": "cross-env NODE_ENV=production yarn run build",
		"build": "babel src --out-dir . --extensions '.ts' --ignore '**/__tests__,**/__mocks__'",
		"watch": "babel src --out-dir . --extensions '.ts' --ignore '**/__tests__,**/__mocks__'",
		"test": "jest"
	},
	"dependencies": {
		"@medusajs/cache-inmemory": "^1.8.0",
		"@medusajs/cache-redis": "1.8.0",
		"@medusajs/event-bus-local": "^1.8.0",
		"@medusajs/event-bus-redis": "1.8.0",
		"@medusajs/medusa": "1.8.0",
		"@sanity/client": "^5.4.2",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.2",
		"typeorm": "0.3.11"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.4",
		"@babel/node": "^7.7.4",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-transform-instanceof": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-typescript": "^7.21.4",
		"@babel/register": "^7.7.4",
		"@babel/runtime": "^7.9.6",
		"@medusajs/medusa-js": "^2.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.8",
		"axios": "^1.3.5",
		"cross-env": "^5.2.1",
		"dotenv": "^16.0.3",
		"eslint": "^6.8.0",
		"jest": "^27.3.1",
		"mongoose": "^5.13.14",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.0.7",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.2",
		"medusa-interfaces": "^1.3.6"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsconfig": "tsconfig.json"
			}
		},
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/node_modules/"
		],
		"rootDir": "src",
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
		"transform": {
			".ts": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
